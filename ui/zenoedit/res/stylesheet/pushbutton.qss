	color: #8A8A8C;
}

QPushButton[cssClass="timeline"]
{
	padding-top: 4px;
	padding-left: 12px;
	padding-right: 12px;
	padding-bottom: 4px;
	border: 1px solid rgba(0,0,0,1);
    border-radius: 4px;
	background-color: #2B2B2B;
	font: bold 14px 'HarmonyOS Sans';
	color: #8A8A8C;
}

QPushButton[cssClass="timeline"]:pressed
{
	background-color: rgb(26, 26, 26);
	color: #8A8A8C;
}

QPushButton[cssClass="timeline"]#btnPlay:pressed
{
	background-color: rgb(26, 26, 26);
	color: #8A8A8C;
}

QPushButton[cssClass="timeline"]#btnPlay:checked
{
	background-color: rgb(26, 26, 26);
	color: #8A8A8C;
}

