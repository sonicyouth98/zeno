QPushButton[cssClass="grayButton"]
{
    border-width: 0px;
    border-radius: 0px;
	background-color: rgb(71,71,71);
	font: 14px 'HarmonyOS Sans';
	color: rgb(168, 163, 160);
}

QPushButton[cssClass="grayButton"]:hover
{
	color: rgb(255, 255, 255);
}

QPushButton[cssClass="welcomepage"]
{
	border-style: solid;
    border-width: 1px;
    border-radius: 0px;
    border-color: rgba(133,130,128,1);
	background-color: transparent;
	font: 14px 'HarmonyOS Sans';
	color: rgb(156, 156, 156);
}

QPushButton[cssClass="welcomepage"]:hover
{
    border-color: rgb(255,255,255);
	color: rgb(255, 255, 255);
}

QPushButton[cssClass="shadowButton"]
{
	padding-top: 2.5px;
	padding-left: 6px;
	padding-right: 6px;
	padding-bottom: 2.5px;
	border: 0.5px solid rgba(0,0,0,1);
    border-radius: 3px;
	background-color: #2B2B2B;
	font: 14px 'HarmonyOS Sans';
	color: #8A8A8C;
}

QPushButton[cssClass="shadowButton"]:pressed
{
	background-color: #000000;
	color: #8A8A8C;
}

QPushButton#btnFree
{
	border: 0.5px solid rgba(0,0,0,1);
    border-radius: 2px;
	background-color: #2B2B2B;
	background-image: url(:/icons/ic_handle_free.svg);
	background-position: center;
	background-repeat: no-repeat;
	padding-left: 6px;
	padding-top: 2.5px;
	padding-right: 6px;
	padding-bottom: 2.5px;
}

QPushButton#btnFree:pressed, QPushButton#btnFree:checked {
    background-color: rgb(26, 26, 26);
	background-image: url(:/icons/ic_handle_free-on.svg);
}

