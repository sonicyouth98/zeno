QPushButton[cssClass="curve-preset"]
{
	padding-top: 2px;
	padding-left: 10px;
	padding-right: 10px;
	padding-bottom: 2px;
	border: 0.5px solid rgba(0,0,0,1);
    border-radius: 3px;
	background-color: #2B2B2B;
	font: 12px 'HarmonyOS Sans';
	color: #8A8A8C;
}

QPushButton[cssClass="curve-preset"]:pressed
{
